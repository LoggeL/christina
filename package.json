{
  "name": "ciri-bot",
  "version": "0.42.0",
  "description": "the discord bot that somehow works",
  "main": "dist/bot/index.js",
  "scripts": {
    "compile:watch": "npm run pre-compile && tsc -w",
    "pre-commit": "npm run compile && npm run lint:fix",
    "tsc": "tsc",
    "compile": "npm run pre-compile && npm run tsc",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "lint": "eslint ./src --ext .ts",
    "pre-compile": "rm -rf dist",
    "pre-start": "npm run lint:fix && npm run compile",
    "start": "npm run pre-start && node -r dotenv/config ."
  },
  "author": "Lukasitzzy",
  "license": "MIT",
  "dependencies": {
    "@discordjs/form-data": "^3.0.1",
    "@types/color-name": "^1.1.1",
    "@types/node": "^14.14.27",
    "@types/node-fetch": "^2.5.8",
    "@types/ws": "^7.4.0",
    "chalk": "^4.1.0",
    "discord-akairo": "github:discord-akairo/discord-akairo",
    "discord.js": "github:discordjs/discord.js",
    "dotenv": "^8.2.0",
    "moment": "^2.29.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.20.0"
  }
}
